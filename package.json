    "@loadable/component": "^5.15.3",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "antd": "^5.1.7",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "chance": "^1.1.7",
    "chart.js": "^3.2.1",
    "class-validator": "^0.13.1",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.4",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "humps": "^2.0.1",
    "i18next": "^19.9.2",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "qs": "^6.9.6",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.3",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "^3.1.3",
    "react-final-form-hooks": "^2.0.2",
    "react-final-form-listeners": "^1.0.3",
    "react-i18next": "^11.8.15",
    "react-intersection-observer": "^9.4.1",
    "react-query": "^3.12.2",
    "react-query-devtools": "3.0.0-beta.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "recharts": "^2.0.9",
    "typescript": "~4.2.4",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fb:init": "firebase init hosting",
    "fb:deploy": "yarn build && firebase deploy --only hosting",
    "semantic-release": "env-cmd -f .env semantic-release --no-ci"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/humps": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.1",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/react-final-form-listeners": "^1.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/recharts": "^1.8.19",
    "conventional-changelog-eslint": "^3.0.9",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "semantic-release": "^20.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acelectic/tom-tom-manager-react.git"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepare": "echo ${nextRelease.version} > VERSION"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}